<?xml version='1.0' encoding='utf-8'?>
<interface version="1.0">
  <object class="tk.Frame" id="bottom">
    <property name="height">100</property>
    <property name="width">100</property>
    <layout manager="grid">
      <property name="propagate">True</property>
      <property name="row">2</property>
      <property name="sticky">s</property>
    </layout>
    <child>
      <object class="ttk.Progressbar" id="progress_bar">
        <property name="length">300</property>
        <property name="orient">horizontal</property>
        <layout manager="pack">
          <property name="pady">12</property>
          <property name="propagate">True</property>
          <property name="side">left</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="button_2">
        <property name="command">process_btn_click</property>
        <property name="text" translatable="yes">Process</property>
        <layout manager="pack">
          <property name="padx">5</property>
          <property name="propagate">True</property>
          <property name="side">left</property>
        </layout>
      </object>
    </child>
  </object>
  <object class="tk.Frame" id="menubar">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="grid">
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
      <property name="sticky">nw</property>
    </layout>
    <child>
      <object class="ttk.Menubutton" id="file">
        <property name="text" translatable="yes">File</property>
        <layout manager="pack">
          <property name="anchor">w</property>
          <property name="propagate">True</property>
          <property name="side">left</property>
        </layout>
        <child>
          <object class="tk.Menu" id="menu_1">
            <property name="tearoff">false</property>
            <child>
              <object class="tk.Menuitem.Command" id="config">
                <property name="command">open_config</property>
                <property name="command_id_arg">false</property>
                <property name="label" translatable="yes">Configuration</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Menubutton" id="edit">
        <property name="text" translatable="yes">Edit</property>
        <layout manager="pack">
          <property name="anchor">w</property>
          <property name="propagate">True</property>
          <property name="side">left</property>
        </layout>
        <child>
          <object class="tk.Menu" id="menu_2">
            <property name="tearoff">false</property>
            <child>
              <object class="tk.Menuitem.Command" id="command_2">
                <property name="command_id_arg">false</property>
                <property name="label" translatable="yes">command_2</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
